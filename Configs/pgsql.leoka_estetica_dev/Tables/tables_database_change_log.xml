<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

  <include file="[Configs].[ProjectColumnsNames]/table_change_log.xml" relativeToChangelogFile="true"/>
  <include file="[Configs].[ProjectVacancyColumnsNames]/table_change_log.xml" relativeToChangelogFile="true"/>
  <include file="[Configs].[ValidationColumnsExclude]/table_change_log.xml" relativeToChangelogFile="true"/>
  <include file="[Configs].[ProjectTeamColumnsNames]/table_change_log.xml" relativeToChangelogFile="true"/>
  
</databaseChangeLog>